{
    "presets": ["@babel/preset-env", "@babel/typescript", "@babel/react"],
    "plugins": [
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread",
        [
            "@babel/plugin-transform-runtime",
            {
                "corejs": false,
                "regenerator": true
            }
        ],
        "babel-plugin-styled-components"
    ]
}
